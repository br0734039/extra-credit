#include <iostream>
#include <vector>

using namespace std;

int main(){
    vector<int> vec;
    long long int x=600851475143;
    int i = 2;
    while (x>1){
        if (x%i!=0)
            i++;
        else
            x/=i;
            vec.push_back(i);
    }
    cout << vec[vec.size()-1];
    return 0;
}
